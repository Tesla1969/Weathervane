<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edgesoftware.weathervane">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FarmStandDashboardActivity"
            android:label="@string/farm_stands"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FarmStandListActivity"
            android:label="Edit Farm Stand"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditFarmResourceActivity"
            android:label="@string/title_activity_edit_farm_resource" />
    </application>

</manifest>